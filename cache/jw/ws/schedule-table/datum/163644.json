系我。\r\n15974552692 (微信同号）\r\n王洪波\r\nwangh@ustc.edu.cn", "scheduleRemark": null, "adminclasses": [], "scheduleJsonParams": [], "selectedStdCount": 0}], "scheduleList": [{"lessonId": 163644, "scheduleGroupId": 179889, "periods": 4.0, "date": "2025-02-25", "room": {"id": 127, "nameZh": "3A310", "nameEn": "3A310", "code": "3A310", "building": {"id": 4, "nameZh": "第三教学楼", "nameEn": "NO.3", "code": "3", "campus": {"id": 7, "nameZh": "西区", "nameEn": "west campus", "code": "3"}}, "roomType": {"id": 24, "nameZh": "多媒体教室", "nameEn": "Multimedia Classroom", "code": "2"}, "floor": 3, "virtual": false, "seatsForLesson": 60, "remark": "四块黑板", "seats": 0}, "weekday": 2, "startTime": 750, "endTime": 1120, "teacherId": 6045, "personId": 80063, "personName": "王洪波", "experiment": null, "customPlace": null, "lessonType": null, "weekIndex": 1, "exerciseClass": false, "startUnit": 0, "endUnit": 0}, {"lessonId": 163644, "scheduleGroupId": 179889, "periods": 4.0, "date": "2025-03-04", "room": {"id": 127, "nameZh": "3A310", "nameEn": "3A310", "code": "3A310", "building": {"id": 4, "nameZh": "第三教学楼", "nameEn": "NO.3", "code": "3", "campus": {"id": 7, "nameZh": "西区", "nameEn": "west campus", "code": "3"}}, "roomType": {"id": 24, "nameZh": "多媒体教室", "nameEn": "Multimedia Classroom", "code": "2"}, "floor": 3, "virtual": false, "seatsForLesson": 60, "remark": "四块黑板", "seats": 0}, "weekday": 2, "startTime": 750, "endTime": 1120, "teacherId": 6045, "personId": 80063, "personName": "王洪波", "experiment": null, "customPlace": null, "lessonType": null, "weekIndex": 2, "exerciseClass": false, "startUnit": 0, "endUnit": 0}, {"lessonId": 163644, "scheduleGroupId": 179889, "periods": 4.0, "date": "2025-03-11", "room": {"id": 127, "nameZh": "3A310", "nameEn": "3A310", "code": "3A310", "building": {"id": 4, "nameZh": "第三教学楼", "nameEn": "NO.3", "code": "3", "campus": {"id": 7, "nameZh": "西区", "nameEn": "west campus", "code": "3"}}, "roomType": {"id": 24, "nameZh": "多媒体教室", "nameEn": "Multimedia Classroom", "co